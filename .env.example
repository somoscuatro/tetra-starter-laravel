# -----------------------------------------------------------------
# Application Settings
# -----------------------------------------------------------------
APP_NAME="TetraStarter Laravel"
APP_ENV=local
APP_URL=https://localhost

# -----------------------------------------------------------------
# Localizations
# -----------------------------------------------------------------
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_TIMEZONE=UTC

# -----------------------------------------------------------------
# Debug & Logging
# -----------------------------------------------------------------
APP_DEBUG=true

DEBUGBAR_ENABLED=true

SAIL_XDEBUG_MODE=develop,debug
#SAIL_XDEBUG_MODE=develop,debug,coverage,profile,trace

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# -----------------------------------------------------------------
# Security Keys
# -----------------------------------------------------------------
APP_KEY=

# -----------------------------------------------------------------
# Maintenance & Workers
# -----------------------------------------------------------------
APP_MAINTENANCE_DRIVER=file
PHP_CLI_SERVER_WORKERS=4

# -----------------------------------------------------------------
# Hashing
# -----------------------------------------------------------------
BCRYPT_ROUNDS=12

# -----------------------------------------------------------------
# Database
# -----------------------------------------------------------------
DB_CONNECTION=pgsql
DB_HOST=pgsql
DB_PORT=5432
DB_DATABASE=laravel
DB_USERNAME=sail
DB_PASSWORD=password

# -----------------------------------------------------------------
# Cache
# -----------------------------------------------------------------
CACHE_STORE=redis
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# -----------------------------------------------------------------
# Session
# -----------------------------------------------------------------
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# -----------------------------------------------------------------
# Queue & Broadcasting
# -----------------------------------------------------------------
QUEUE_CONNECTION=redis
BROADCAST_CONNECTION=log

# -----------------------------------------------------------------
# Mail
# -----------------------------------------------------------------
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# -----------------------------------------------------------------
# Vite
# -----------------------------------------------------------------
VITE_APP_NAME="${APP_NAME}"

# -----------------------------------------------------------------
# Scout & MeiliSearch
# -----------------------------------------------------------------
SCOUT_DRIVER=meilisearch

MEILISEARCH_HOST=http://meilisearch:7700
MEILISEARCH_NO_ANALYTICS=false

# -----------------------------------------------------------------
# Media & File Storage
# -----------------------------------------------------------------
FILESYSTEM_DISK=local

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false
